<?xml version="1.0" encoding="UTF-8"?>
<testcases name="PositionScenarios">
   <property name="source.folder"
             value="${project.folder}/regressiontest/source/PositionSc"/>
   <property name="reference.folder"
             value="${project.folder}/regressiontest/reference/PositionSc"/>
   <property name="output.folder"
             value="${project.folder}/regressiontest/.$output/PositionSc"/>
   <testcase name="positioncheck" type="TestcaseTest">
      <options>
         <option directory="true"
                 name="run.testcase.test.output.folder"
                 value="${output.folder}/BuilderUMLService.NameOfPortType1.operation1/positioncheck"/>
         <option directory="true"
                 name="run.testcase.test.testcase.home"
                 value="${project.folder}/regressiontest/source/PositionSc"/>
         <option name="run.testcase.test.testcase" value="positioncheck"/>
         <option name="key.option.test.name" value="positioncheck"/>
         <option name="run.testcase.test.operation"
                 value="BuilderUMLService.NameOfPortType1.operation1"/>
         <option name="run.testcase.test.host" value="localhost:11165"/>
      </options>
      <compare>
         <testcaseCompare compareType="equal" name="Compare run of positioncheck">
            <option directory="true"
                    name="xml.compare.reference.file"
                    value="${reference.folder}/BuilderUMLService.NameOfPortType1.operation1/positioncheck"/>
            <option directory="true"
                    name="xml.compare.test.file"
                    value="${output.folder}/BuilderUMLService.NameOfPortType1.operation1/positioncheck"/>
         </testcaseCompare>
      </compare>
   </testcase>
   <testcase name="positioncheck2" type="TestcaseTest">
      <options>
         <option directory="true"
                 name="run.testcase.test.output.folder"
                 value="${output.folder}/BuilderUMLService.NameOfPortType1.operation1/positioncheck2"/>
         <option directory="true"
                 name="run.testcase.test.testcase.home"
                 value="${project.folder}/regressiontest/source/PositionSc"/>
         <option name="run.testcase.test.testcase" value="positioncheck2"/>
         <option name="key.option.test.name" value="positioncheck2"/>
         <option name="run.testcase.test.operation"
                 value="BuilderUMLService.NameOfPortType1.operation1"/>
         <option name="run.testcase.test.host" value="localhost:11165"/>
      </options>
      <compare>
         <testcaseCompare compareType="equal" name="Compare run of positioncheck2">
            <option directory="true"
                    name="xml.compare.reference.file"
                    value="${reference.folder}/BuilderUMLService.NameOfPortType1.operation1/positioncheck2"/>
            <option directory="true"
                    name="xml.compare.test.file"
                    value="${output.folder}/BuilderUMLService.NameOfPortType1.operation1/positioncheck2"/>
         </testcaseCompare>
      </compare>
   </testcase>
</testcases>
