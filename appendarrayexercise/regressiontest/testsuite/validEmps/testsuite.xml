      <compare>
         <testcaseCompare compareType="equal" name="Compare run of addemp2">
            <option directory="true"
                    name="xml.compare.reference.file"
                    value="${reference.folder}/BuilderUMLService.NameOfPortType1.operation1/addemp2"/>
            <option directory="true"
                    name="xml.compare.test.file"
