                 value="BuilderUMLService.NameOfPortType1.operation1"/>
         <option name="run.testcase.test.host" value="localhost:11184"/>
      </options>
      <compare>
         <testcaseCompare compareType="equal" name="Compare run of blockedemp">
            <option directory="true"
                    name="xml.compare.reference.file"
                    value="${reference.folder}/BuilderUMLService.NameOfPortType1.operation1/blockedemp"/>
            <option directory="true"
                    name="xml.compare.test.file"
                    value="${library.output.folder}/BuilderUMLService.NameOfPortType1.operation1/blockedemp"/>
