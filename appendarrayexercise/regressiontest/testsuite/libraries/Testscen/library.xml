         <option directory="true"
                 name="run.testcase.test.output.folder"
                 value="${library.output.folder}/BuilderUMLService.NameOfPortType1.operation1/blockedemp"/>
         <option directory="true"
                 name="run.testcase.test.testcase.home"
                 value="${project.folder}/regressiontest/source/libraries/Testscen"/>
         <option name="run.testcase.test.testcase" value="blockedemp"/>
         <option name="key.option.test.name" value="blockedemp"/>
